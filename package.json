{
  "name": "oxbot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:svelte": "vite build",
    "build:ts": "npm run --prefix functions build",
    "build": "npm run build:ts && npm run build:svelte",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev:fb": "kill-port 8080 && firebase emulators:start --import=cache --export-on-exit",
    "dev:svelte": "vite",
    "dev:ts": "npm run --prefix functions dev",
    "dev": "concurrently -c yellow,red,blue \"npm:dev:*\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tsconfig/svelte": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-svelte": "^2.26.0",
    "firebase-tools": "^12.4.6",
    "kill-port": "^2.0.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "firebase": "^10.1.0"
  }
}
